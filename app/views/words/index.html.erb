<h1>Word Bank</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @words.each do |word| %>
      <tr>
        <td><%= link_to word.name, word, :id => word.id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>